    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.1.1",
    "raw-loader": "^0.5.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
