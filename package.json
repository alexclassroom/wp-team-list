{
  "name": "wp-team-list",
  "description": "Create a simple team site using your WordPress users. The plugin provides you with a shortcode, template tag and a widget.",
  "homepage": "https://required.com/services/wordpress-plugins/wp-team-list/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearerequired/wp-team-list"
  },
  "license": "GPL-2.0-or-later",
  "author": {
    "name": "required",
    "email": "support@required.ch",
    "url": "https://required.com"
  },
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:css": "concurrently \"npm:build:css:*\"",
    "build:css:frontend": "postcss assets/css/src/style.css -o assets/css/style.css",
    "build:js": "NODE_ENV=production webpack",
    "dev": "concurrently \"npm:watch:*\"",
    "lint": "eslint .",
    "rtlcss": "concurrently \"npm:rtlcss:*\"",
    "rtlcss:editor": "rtlcss assets/css/editor.css assets/css/editor-rtl.css",
    "rtlcss:frontend": "rtlcss assets/css/style.css assets/css/style-rtl.css",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:css": "NODE_ENV=development nodemon --watch assets/css/src -e css -x 'npm run build:css'",
    "watch:js": "webpack --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.4",
    "@wordpress/api-fetch": "^3.3.0",
    "@wordpress/babel-preset-default": "^4.3.0",
    "@wordpress/blocks": "^6.4.0",
    "@wordpress/components": "^8.0.0",
    "@wordpress/data": "^4.6.0",
    "@wordpress/editor": "^9.4.0",
    "@wordpress/element": "^2.5.0",
    "@wordpress/i18n": "^3.5.0",
    "@wordpress/url": "^2.6.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "css-loader": "^3.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint-config-wordpress": "^2.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "postcss-hexrgba": "^1.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^3.0.0",
    "rtlcss": "^2.4.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "accessible-autocomplete": "^1.6.2",
    "react-select": "^3.0.0"
  }
}
